version 42
__lua__
-- init

function _init()
	player = {
		sp = 1,
		x = 0,
		y = 4,
		acc = 1
	}
end


-->8
-- update and draw

function _update()
	move_player()
end

function _draw()
	cls()
	map()
	spr(player.sp, player.x, player.y)
	print(player.x, 100, 0)
	print(player.y, 120, 0)
end
-->8
-- player
function move_player()
	if btn(⬅️) then
		player.x -= player.acc
	end
	
	if btn(➡️) then
		player.x += player.acc
	end
	
	if btn(⬆️) then
		player.y -= player.acc
	end
	
	if btn(⬇️) then
		player.y += player.acc
	end
end
